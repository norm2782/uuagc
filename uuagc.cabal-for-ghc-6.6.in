name: uuagc
version: @VERSION@
license: GPL
license-file: LICENSE
maintainer: Arie Middelkoop <ariem@cs.uu.nl>
Build-Depends: base, haskell98, uulib, mtl

Executable:     uuagc
Main-Is:        Ag.hs
Other-Modules: AbstractSyntax,
      Code,
      CommonTypes,
      ConcreteSyntax,
      DefaultRules,
      DepTypes,
      ErrorMessages,
      Expression,
      GenerateCode,
      HsToken,
      HsTokenScanner,
      Options,
      Parser,
      Patterns,
      PrintCode,
      PrintErrorMessages,
      Scanner,
      SemHsTokens,
      Streaming,
      TokenDef,
      Transform,
      Version
Extensions: TypeSynonymInstances, MultiParamTypeClasses
Hs-Source-Dirs: src
