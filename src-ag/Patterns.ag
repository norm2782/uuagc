imports
{
import UU.Scanner.Position(Pos)
import CommonTypes
}
TYPE Patterns   = [Pattern]

DATA Pattern    | Constr     name  : {Constructor}
                             pats  : Patterns
                | Product    pos   : {Pos}
                             pats  : Patterns
                | Alias      field : {Name}
                             attr  : {Name}
                             pat   : Pattern
                | Underscore pos   : {Pos}