imports
{
import CommonTypes
import UU.Scanner.Position(Pos)
}

DATA HsTokensRoot
   | HsTokensRoot tokens : HsTokens

TYPE HsTokens = [HsToken]

DATA HsToken
   | AGLocal   var   : {Name}
               pos   : {Pos}

   | AGField   field : {Name}
               attr  : {Name}
               pos   : {Pos}

   | HsToken   value : {String}
               pos   : {Pos}
   | CharToken value : {String}
               pos   : {Pos}
   | StrToken  value : {String}
               pos   : {Pos}
   | Err       mesg  : {String}
               pos   : {Pos}